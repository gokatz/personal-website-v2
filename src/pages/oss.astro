---
// Component Imports
import Shell from '../layouts/Shell.astro';
import BlogTitle from '../components/BlogTitle.astro';

// Data Fetching: List all Markdown posts in the repo.

let allOSSProjects = await Astro.fetchContent('./oss/*.md');
allOSSProjects = allOSSProjects.sort((a, b) => new Date(b.date).valueOf() - new Date(a.date).valueOf());
---

<Shell>
	{
		allOSSProjects.map((blog) => {
			return <BlogTitle blog={blog} /> 
		})
	}
</Shell>